		"tf:apply-dev": "cd terraform && terraform apply -var-file=dev.tfvars",
		"tf:apply-prod": "cd terraform && terraform apply -var-file=prod.tfvars"
  },
  "devDependencies": {
    "clean-webpack-plugin": "^4.0.0",
    "eslint": "^7.1.0",
