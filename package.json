  },
  "scripts": {
    "test": "jest",
    "lint": "eslint --fix .",
    "prettier": "prettier --write .",
    "build": "webpack --mode=production",
		"tf:plan": "cd terraform && terraform plan",
		"tf:destroy-dev": "cd terraform && terraform destroy -var-file=dev.tfvars",
